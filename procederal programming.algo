
function read_vector(i,j,n:INTEGER, v1,v2: array of integer)
var i: Integer
    j:Integer
    n:Integer
begin
for i from 0 to n step 1 do
Read (v1[i])
end for
for j from 0 to n step 1 do
Read (v2[j])
end for
end

//////

PROCEDURE dot_product (PS, v1, v2)
var
PS=0
v1, v2: Array_of_INTEGER
i,j,n:INTEGER
begin
for i from 0 to n step 1 and  j from 0 to n step 1 do
  PS=Ps + v1[i]*v2[j]
  if PS=0 then
     Write (v1 and v2 are orthogonal)
   else
     Write (v1 and v2 are not orthogonal)
end for
end

